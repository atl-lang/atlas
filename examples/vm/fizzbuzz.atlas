// FizzBuzz - count fizz, buzz, and fizzbuzz occurrences
let fizz = 0;
let buzz = 0;
let fizzbuzz = 0;
let i = 1;
while (i <= 100) {
    if (i % 15 == 0) {
        fizzbuzz = fizzbuzz + 1;
    } else {
        if (i % 3 == 0) {
            fizz = fizz + 1;
        } else {
            if (i % 5 == 0) {
                buzz = buzz + 1;
            }
        }
    }
    i = i + 1;
}
fizzbuzz;
