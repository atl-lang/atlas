// Simple state machine simulation
let state = 0;
let output = 0;
let i = 0;
while (i < 10) {
    if (state == 0) {
        state = 1;
        output = output + 1;
    } else {
        if (state == 1) {
            state = 2;
            output = output + 10;
        } else {
            state = 0;
            output = output + 100;
        }
    }
    i = i + 1;
}
output;
